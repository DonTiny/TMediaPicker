<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="11.5dp">
        <rotate
            android:fromDegrees="315"
            android:pivotX="45%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/tMediaPickerToolbarPrimaryReturn" />
                <size
                    android:width="20dp"
                    android:height="3dp" />
            </shape>
        </rotate>
    </item>
    <item android:top="11.5dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="45%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/tMediaPickerToolbarPrimaryReturn" />
                <size
                    android:width="20dp"
                    android:height="3dp" />
            </shape>
        </rotate>
    </item>
</layer-list>
